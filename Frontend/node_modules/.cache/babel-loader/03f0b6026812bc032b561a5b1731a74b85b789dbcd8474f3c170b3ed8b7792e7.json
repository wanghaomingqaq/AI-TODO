registerPassword\" v-model=\"registerPassword\" required>\r\n      <button type=\"submit\">Register</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'LoginForm',\r\n  data() {\r\n    return {\r\n      loginId: '',\r\n      loginPassword: '',\r\n      registerUsername: '',\r\n      registerPassword: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async loginUser() {\r\n      try {\r\n        const response = await axios.post('/login', {\r\n          id: this.loginId,\r\n          password: this.loginPassword,\r\n          msgid: 1\r\n        });\r\n        this.$emit('login-success', response.data);\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    },\r\n    async registerUser() {\r\n      try {\r\n        const response = await axios.post('/reg', {\r\n          msgid:3,\r\n          name: this.registerUsername,\r\n          password: this.registerPassword,\r\n        });\r\n        this.$emit('register-success', response.data);\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,EAAE;MACjBC,gBAAgB,EAAE,EAAE;MACpBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAASA,CAAA,EAAG;MAChB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,QAAQ,EAAE;UAC1CC,EAAE,EAAE,IAAI,CAACR,OAAO;UAChBS,QAAQ,EAAE,IAAI,CAACR,aAAa;UAC5BS,KAAK,EAAE;QACT,CAAC,CAAC;QACF,IAAI,CAACC,KAAK,CAAC,eAAe,EAAEL,QAAQ,CAACP,IAAI,CAAC;MAC5C,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB;IACF,CAAC;IACD,MAAME,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMR,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,MAAM,EAAE;UACxCG,KAAK,EAAC,CAAC;UACPZ,IAAI,EAAE,IAAI,CAACI,gBAAgB;UAC3BO,QAAQ,EAAE,IAAI,CAACN;QACjB,CAAC,CAAC;QACF,IAAI,CAACQ,KAAK,CAAC,kBAAkB,EAAEL,QAAQ,CAACP,IAAI,CAAC;MAC/C,EAAE,OAAOa,KAAK,EA